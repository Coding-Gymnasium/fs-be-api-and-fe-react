{
  "name": "be-api",
  "version": "1.0.0",
  "description": "google auth nodejs express api",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "dev": "nodemon --exec babel-node src/index.js",
    "clean": "rm -rf ./build && mkdir build",
    "prebuild": "npm run clean",
    "build": "babel src -d ./build --ignore ./node_modules",
    "db:setup": "sequelize db:create",
    "db:migrate:up": "sequelize db:migrate",
    "db:migrate:down": "sequelize db:migrate:undo:all",
    "db:refresh": "npm run db:migrate:down && npm run db:migrate:up"
  },
  "author": "Nico",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.2",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.10.0",
    "passport": "^0.5.2",
    "passport-google-token": "^0.1.2",
    "pg": "^8.7.1",
    "sequelize": "^6.12.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.7",
    "@babel/core": "^7.16.7",
    "@babel/node": "^7.16.7",
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/register": "^7.16.7",
    "@babel/runtime": "^7.16.7",
    "babel-node": "^0.0.1-security",
    "nodemon": "^2.0.15",
    "sequelize-cli": "^6.3.0"
  }
}
